int materias(int valor){
    valor=rand()%6+1;
    if(valor==1)
    return 1;
    else if(valor==2)
    return 2;
    else if(valor==3)
    return 3;
    else if(valor==4)
    return 4;
    else if(valor==5)
    return 5;
    else if(valor==6)
    return 6;
}
