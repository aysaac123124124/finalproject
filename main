#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int a,d,materia,valor;
int materias(int valor);
main()
{
    materia=materias(valor);
switch(materia){
    
    case 1:
    printf("Ciencia");
    break;
    case 2:
    printf("Historia");
    break;
    case 3:
    printf("Cultura General");
    break;
    case 4:
    printf("Arte");
    break;
    case 5:
    printf("Geografia");
    break;
    case 6:
    printf("Deportes");
    break;
}

    
}
